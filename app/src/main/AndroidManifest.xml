<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.profitness">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.ACCESS_NOTIFICATION_POLICY" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Profitness">
        <activity android:name=".ShowPerformance"/>
        <activity android:name=".EditPerformance" />
        <activity android:name=".personalInformationDisplayToCoach" />
        <activity android:name=".CoachSeeAllComingTreining" />
        <activity android:name=".CoachPractisTimeOfUser"/>
        <activity android:name=".ShowMenuListCoach" />
        <activity android:name=".Menu" />
        <activity android:name=".SelfCoachScreen" />
        <activity android:name=".SchedAvailability" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".register" />
        <activity android:name=".CoachListActivity" />
        <activity android:name=".DetailsTraining" />
        <activity android:name=".trainee_main_activity" />
        <activity android:name=".Calander" />
        <activity android:name=".WorkoutHistoryLayout" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
        <service
            android:name=".objects.MyFirebaseInstanceIDService">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>
        <service
            android:name=".objects.MyFirebaseMessagingService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
    </application>


</manifest>